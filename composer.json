{
    "name": "tuanbtre/csm",
    "description": "Website for developer",
	"authors": [{
            "name": "Nguyen Minh Tuan",
            "email": "tuancsharp@gmail.com"
        }],
    "require": {
		"php": "^8.0",
		"laravel/ui": "^4.2",
		"spatie/laravel-analytics": "^5.0",
        "unisharp/laravel-filemanager": "^2.5"
	},
	"extra": {
        "laravel": {
            "providers": [
                "Tuanbtre\\Csm\\CsmServiceProvider"
            ]
        }
    },
	"autoload": {
        "psr-4": {
            "Tuanbtre\\Csm\\": "src/",
            "App\\": "app/"
        },
		"files":[
            "src/helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
			"Tuanbtre\\Csm\\": "src/",
			"Tests\\": "tests/"
        }
    }
}
